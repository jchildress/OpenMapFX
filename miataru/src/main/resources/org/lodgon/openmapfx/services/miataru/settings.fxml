<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane prefWidth="400.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
        <Label text="CLIENT SETTINGS" GridPane.columnSpan="2" />
            <HBox prefHeight="21.0" prefWidth="197.0" GridPane.columnSpan="2" GridPane.rowIndex="1">
      <children>
                  <Label text="Track and Report Location" />
                  <Pane prefHeight="1.0" prefWidth="1.0" HBox.hgrow="ALWAYS" />
                  <CheckBox fx:id="track" alignment="TOP_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" textAlignment="CENTER" />
      </children>
            </HBox>
            <Label text="Devicename" GridPane.rowIndex="2" />
            <TextField fx:id="deviceName" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button mnemonicParsing="false" onAction="#applyChanges" text="Apply" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
         </children>
         <padding>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </padding>
      </GridPane>
   </children>
</fx:root>
